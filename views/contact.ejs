<!DOCTYPE html>
<html lang="en">
    <head>
        <meta charset="UTF-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <title>Contact</title>
        <style> 
            .agent {
                background-color: #274d69;
                color: aliceblue;
                line-height: 170%;
                padding: 50px;


            }
            row {
                width:20vw;
            }

        </style>
        <link rel="stylesheet" href="templateStyle.css" />
    </head>
    <body>
        <%- include('./partials/header'); %>
    
        <table style="padding-top:100px">
        <% agencies.forEach(function(agency) { %>
            <tr class="agent row">
                <td class="td1"> <%= agency.AgncyPhone %> </td>
                <td class="td1"> <%= agency.AgncyCity %>, <%= agency.AgncyAddress %>  </td>


            </tr> 

        <% }) %>
        </table>
      <br/><br/>
        <table >
    
        <tr class="agent ">
            <th> Name </th>
            <th> Email </th>
            <th> Phone Number </th>

        </tr>
    
    

        <% agents.forEach(function(agent) { %>
            <tr class="agent">
            <td class="td1  row"><%= agent.AgtFirstName %> <%= agent.AgtLastName %></td>
            <td  class="td1  row"><%= agent.AgtEmail %></td>
            <td class="td1  row" ><%= agent.AgtBusPhone %></td>
        </tr>   
        <% }) %>

        </table>
        <%- include('./partials/footer'); %>
    </body>
</html>